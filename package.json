        "@parcel/core": "^2.9.3",
        "@parcel/namer-default": "^2.9.3",
        "@parcel/optimizer-css": "^2.9.3",
        "@parcel/optimizer-htmlnano": "^2.9.3",
        "@parcel/optimizer-swc": "^2.9.3",
        "@parcel/packager-css": "^2.9.3",
        "@parcel/packager-html": "^2.9.3",
        "@parcel/packager-js": "^2.9.3",
